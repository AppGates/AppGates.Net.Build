<Project>
	<PropertyGroup>
		<!--NU5110: allow powershell scripts within the build folders.-->
		<NoWarn>$(NoWarn);NU5110;NU5111</NoWarn>
	</PropertyGroup>
  <ItemGroup>
    <None Update="buildMultiTargeting\**\*">
      <PackagePath>/</PackagePath>
      <Pack>true</Pack>
    </None>
    <None Update="Sdk\**\*">
      <PackagePath>/</PackagePath>
      <Pack>true</Pack>
    </None>
    <None Update="build\**\*">
      <PackagePath>/</PackagePath>
      <Pack>true</Pack>
    </None>
    <None Update="buildTransitive\**\*">
      <PackagePath>/</PackagePath>
      <Pack>true</Pack>
    </None>

  </ItemGroup>
</Project>