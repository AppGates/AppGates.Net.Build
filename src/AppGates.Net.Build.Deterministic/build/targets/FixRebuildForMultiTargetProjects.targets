<Project>
  <PropertyGroup>
    <RebuildDependsOn>
      BeforeRebuild;
      Clean;
      Build;
      AfterRebuild;
    </RebuildDependsOn>
  </PropertyGroup>

  <Target Name="BeforeRebuild" />
  <Target Name="Rebuild" DependsOnTargets="$(RebuildDependsOn)"/>
  <Target Name="AfterRebuild" />

</Project>